@model IEnumerable<lec5.Models.courses>

@{
    ViewBag.title = "student add ";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cour_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cour_Hours)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cour_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cour_Hours)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Cour_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Cour_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Cour_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
